<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="duan.server.mapper.AdminMapper">


    <select id="findByUid" resultType="duan.server.entity.Admin">
        SELECT * FROM admin WHERE uid = #{uid}
    </select>
    <select id="findStu" resultType="duan.server.entity.Student">
        SELECT * FROM student WHERE uid = #{uid}
    </select>


</mapper>
